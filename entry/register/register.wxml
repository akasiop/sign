<label style="display: flex;" wx:for-items="{{items}}">
<checkbox value="{{item.name}}" checked="{{item.checked}}"/>{{item.value}}
</label>
</checkbox-group>
</view>

<navigator url="NULL">
<button class="button_1">注册</button>
</navigator>

</view>
